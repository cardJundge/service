/* pages/mine/serverItemList/serverItemList.wxss */

.add_Icon {
  background: #3891F8;
  text-align: right;
  height: 40px;
}

.add_Icon image {
  width: 20px;
  height: 20px;
  margin-right: 15px;
  margin-top: 10px;
}

.each_server {
  border-bottom: 1rpx solid #efefef;
}

.each_content {
  margin: 0px 15px;
  font-size: 14px;
  padding: 20px 0px;
  line-height: 30px;
}

.server_name {
  display: inline-block;
}

.server_name_image {
  display: inline-block;
}

.server_name_image view {
  line-height: 30px;
  display: inline-block;
}

.server_image image {
  width: 40px;
  height: 40px;
  vertical-align: middle;
}
.server_name {
  margin-left: 10px;
}
.server_edit {
  float: right;
  color: #3891f8;
}

.server_edit button {
  display: inline-block;
  color: #3891f8;
  font-size: 14px;
  border:1rpx solid #3891f8;
  border-radius: 200px;
  width:70px;
  line-height: 30px;
  background: none;
}
.server_edit .delete{
  color:#fff;
  background: #3891f8;
  margin-left: 10px;
}
.noData{
  font-size:14px;
  line-height:50px;
  text-align:center;
}