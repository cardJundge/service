@import '../../../index/checkLoss/fix/fix.wxss';

view {
  box-sizing: border-box;
}

.ok_btn {
  position: fixed;
  top: 0px;
  left: 0px;
  display: inline-block;
  width: 100%;
  z-index: 3;
  height: 35px;
  background: linear-gradient(#3891f8, rgb(164, 205, 253));
}

.hasCoor {
  text-align: center;
  position: fixed;
  top: 35px;
  left: 0px;
  display: inline-block;
  width: 100%;
  height: 30px;
  z-index: 2;
}

.hasCoor_cell {
  display: inline-block;
  width: 100%;
  text-align: center;
  background: #eee;
}

.hasCoor_tit {
  display: inline-block;
  text-align: left;
  font-size: 14px;
  color: #3891f8;
  width: 94%;
  line-height: 30px;
  position: relative;
}

.hasCoor_con_cell {
  display: flex;
  text-align: center;
  line-height: 80rpx;
  font-size: 30rpx;
}

.hasCoor_tit image {
  width: 14px;
  height: 8px;
  vertical-align: middle;
  position: absolute;
  right: 20px;
  top: 10px;
}

.service_tit {
  display: inline-block;
  width: 50%;
  vertical-align: top;
  color: #999;
}

.brand_cell_tit {
  display: inline-block;
  width: 50%;
}

.brand_cell_tit text {
  display: inline-block;
  margin-right: 10px;
  font-size: 10px;
}

.zan-icon-contact {
  color: #3891f8;
}

.service_cell {
  display: inline-block;
  width: 50%;
}

.select_icon {
  display: inline-block;
  width: 30%;
  text-align: right;
}

.series_name {
  display: inline-block;
  width: 70%;
  height: 36px;
  vertical-align: top;
  overflow: hidden;
  white-space: nowrap;
  Text-overflow: ellipsis;
}

.modal {
  position: fixed;
  top: 0px;
  left: 0px;
  width: 100%;
  height: 100vh;
  z-index: 3;
  background: rgba(110, 110, 110, 0.5);
  text-align: center;
}

.modal_content {
  text-align: left;
  background: #fff;
  display: inline-block;
  width: 80%;
  border-radius: 5px;
  padding: 20px 0px;
  margin-top: 20%;
}

.modal_title {
  text-align: center;
  font-size: 16px;
  padding-bottom: 10px;
  color: #09bb07;
}

.modal_main {
  font-size: 14px;
  line-height: 40px;
  margin: 0px 20px;
}

.modal_bottom {
  margin-top: 10px;
}

.modal_bottom view {
  font-size: 14px;
  float: right;
}

.cancel {
  margin-right: 50px;
  color: #aaa;
  vertical-align: middle;
}

.ok {
  margin-right: 20px;
  color: #09bb07;
}

.modal_main radio {
  float: right;
}

.modal_bottom button {
  background: #fff;
  color: #09bb07;
  font-size: 14px;
  margin: 0px;
  vertical-align: middle;
}

.modal_bottom button::after {
  border: none;
}

.leftview {
  width: 40%;
  display: flex;
  flex-direction: column;
  background: #f6f6f6;
  box-shadow: -10px 0px 10px -5px #e6e6e6 inset;
  /* display: inline-block; */
}

.rightview {
  width: 58%;
  /* display: inline-block; */
}

.leftlist {
  background: #f6f6f6;
  box-shadow: -10px 0px 10px -5px #e6e6e6 inset;
}

.rightlist {
  font-size: 30rpx;
  border-bottom: 1rpx solid #eee;
}

.activemenu {
  background: #f4f8ff;
}
